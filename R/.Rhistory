geom_point(aes(fill = HybridStatus), size=4, pch = 21, color = "black") +
mytheme +
facet_grid(. ~ infection_isolate) +
scale_x_continuous(breaks = 0:11, name = "Day post infection (dpi)") +
scale_y_continuous(name = "Nbr oocysts shed")
